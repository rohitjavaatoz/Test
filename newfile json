import javax.persistence.*;
import java.sql.Timestamp;

@Entity
@Table(name = "ucmcmt.base_model_entities")
public class BaseModelEntity {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    @Column(name = "base_model_entity_id")
    private Integer baseModelEntityId;

    @Column(name = "start_date")
    private Timestamp startDate;

    @Column(name = "start_date_conf")
    private Integer startDateConf;

    @Column(name = "end_date")
    private Timestamp endDate;

    @Column(name = "end_date_conf")
    private Integer endDateConf;

    @Column(name = "outage_duration")
    private Integer outageDuration;

    @Column(name = "outage_duration_conf")
    private Integer outageDurationConf;

    @Column(name = "outage_duration_uom")
    private String outageDurationUom;

    @Column(name = "outage_duration_uom_conf")
    private Integer outageDurationUomConf;

    @Column(name = "city")
    private String city;

    @Column(name = "city_conf")
    private Integer cityConf;

    @Column(name = "state")
    private String state;

    @Column(name = "state_conf")
    private Integer stateConf;

    @Column(name = "country")
    private String country;

    @Column(name = "country_conf")
    private Integer countryConf;

    @Column(name = "email_message_id")
    private Integer emailMessageId;

    @Column(name = "created_date")
    private Timestamp createdDate;

    @ManyToOne
    @JoinColumn(name = "email_message_id", referencedColumnName = "email_message_id", insertable = false, updatable = false)
    private EmailMessage emailMessage;

    // Constructors, getters, and setters

    // Add constructors, getters, and setters here
}

--------------------------------
CREATE TABLE IF NOT EXISTS ucmcmt.base_model_entities
(
    base_model_entity_id integer NOT NULL DEFAULT nextval('base_model_entities_id_seq'::regclass),
    start_date timestamp without time zone,
    start_date_conf integer,
    end_date timestamp without time zone,
    end_date_conf integer,
    outage_duration integer,
    outage_duration_conf integer,
    outage_duration_uom character varying COLLATE pg_catalog."default",
    outage_duration_uom_conf integer,
    city character varying COLLATE pg_catalog."default",
    city_conf integer,
    state character varying COLLATE pg_catalog."default",
    state_conf integer,
    country character varying COLLATE pg_catalog."default",
    country_conf integer,
    email_message_id integer,
    created_date timestamp without time zone,
    CONSTRAINT base_model_entities_id_pkey PRIMARY KEY (base_model_entity_id),
    CONSTRAINT email_message_id_fkey FOREIGN KEY (email_message_id)
        REFERENCES ucmcmt.email_messages (email_message_id) MATCH SIMPLE
        ON UPDATE NO ACTION
        ON DELETE NO ACTION
)
-----------------------------------------------
import com.fasterxml.jackson.annotation.JsonFormat;

import java.util.Date;
import java.util.List;

public class ChangeRequest {

    private String description;
    private List<ExternalReference> externalReference;
    private Location location;
    
    @JsonFormat(pattern = "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'")
    private Date plannedEndTime;
    
    @JsonFormat(pattern = "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'")
    private Date plannedStartTime;
    
    private List<Attachment> attachment;
    private List<TargetEntity> targetEntity;
    private List<RelatedParty> relatedParty;
    private String requestType;
    private String priority;
    private Specification specification;
    private Outage outage;

    // getters and setters

}

class ExternalReference {
    private String id;
    // getters and setters
}

class Location {
    private String name;
    // getters and setters
}

class Attachment {
    private String href;
    private String name;
    private Size size;
    private String attachmentType;
    // getters and setters
}

class Size {
    private double amount;
    // getters and setters
}

class TargetEntity {
    private String name;
    // getters and setters
}

class RelatedParty {
    private String name;
    private String role;
    private String referredType;
    // getters and setters
}

class Specification {
    private String id;
    private String href;
    private String name;
    private String version;
    private String referredType;
    private String type;
    private String schemaLocation;
    private String baseType;
    // getters and setters
}

class Outage {
    private int outageDuration;
    private String outageDurationUom;
    private int outageNumber;

    @JsonFormat(pattern = "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'")
    private Date outageStart;

    @JsonFormat(pattern = "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'")
    private Date outageEnd;
    // getters and setters
}
